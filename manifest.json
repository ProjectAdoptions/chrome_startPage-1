{

    "manifest_version": 2,
   "name": "ChromeStart",
   "offline_enabled": true,
   "permissions": [ "unlimitedStorage" ],
   "short_name": "ChromeStart",
   "version": "1.3",
   "update_url": "http://dmitry.96.lt/chrome/update.xml",

   "background": {
      "persistent": false,
      "scripts": [ "js/page.js" ]
   },

   "chrome_url_overrides": {
      "newtab": "dashboard.html"
   },

   "description": "Replace new tab page with a personal dashboard.",
   "icons": {
      "128": "icon-128.png",
      "16": "icon-16.png",
      "48": "icon-48.png"
   }

}
